   "source": [
    "from progress.bar import Bar\n",
    "\n",
    "x = range(1,500)\n",
    "\n",
    "bar = Bar('Processing', max= len(x))\n",
    "\n",
    "y = 0\n",
    "\n",
    "def func(num):\n",
    "    return num*2\n",
    "\n",
    "\n",
    "for i in x:\n",
    "    func(i)\n",
    "    bar.next()\n",
    "bar.finish()\n",
    "\n"
   ]
